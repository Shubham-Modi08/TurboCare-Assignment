<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shubham.turbocare_assignment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TurboCareassignment">
        <activity android:name=".activities.VehicleDetails" />
        <activity android:name=".activities.SelectVehicleFuelType" />
        <activity android:name=".activities.SelectVehicleTransmission" />
        <activity android:name=".activities.VehicleList" />
        <activity android:name=".activities.SelectVehicleModel" />
        <activity android:name=".activities.SelectVehicleMake" />
        <activity android:name=".activities.MainActivity" />
        <activity android:name=".activities.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>